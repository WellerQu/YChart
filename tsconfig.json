{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "removeComments": false,
    "target": "ES5",
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es2015",
      "es2015.core"
    ],
  },
  "include": [
    "src/**/*",
    "node_modules/snabbdom/snabbdom.d.ts",
    "node_modules/snabbdom/vnode.d.ts",
    "node_modules/snabbdom/tovnode.d.ts",
  ],
  "exclude": [
    "**/*.spec.ts",
    "src/ychart-callstack.ts"
  ]
}