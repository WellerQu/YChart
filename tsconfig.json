{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "removeComments": false,
    "target": "ES5",
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ES2015",
      "ES2015.Core",
      "ES2016.Array.Include"
    ],
  },
  "include": [
    "src/**/*",
    "node_modules/snabbdom/snabbdom.d.ts",
    "node_modules/snabbdom/vnode.d.ts",
    "node_modules/snabbdom/tovnode.d.ts",
    "node_modules/snabbdom/modules/attributes.d.ts",
    "node_modules/snabbdom/modules/style.d.ts",
    "node_modules/snabbdom/modules/class.d.ts",
    "node_modules/snabbdom/modules/eventlisteners.d.ts"
  ],
  "exclude": [
    "**/*.spec.ts",
    "src/ychart-callstack.ts"
  ]
}