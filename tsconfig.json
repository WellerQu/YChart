{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "removeComments": false,
    "target": "ES5",
    "declaration": true,
    "lib": [
      "dom",
      "es2015",
      "es2015.core"
    ],
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "**/*.spec.ts",
  ]
}